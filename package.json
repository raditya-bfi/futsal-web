    "@mui/lab": "^5.0.0-alpha.91",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "@mui/x-date-pickers": "^5.0.18",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.2",
    "formik": "^2.2.9",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
